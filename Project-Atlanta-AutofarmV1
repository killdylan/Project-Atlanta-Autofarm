 while wait(1.5) do  
tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1.2, Enum.EasingStyle.Linear)
tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-199, 17, -112)})
tween:Play()
fireproximityprompt(game:GetService("Workspace").Restocking.Part.ProximityPrompt)
